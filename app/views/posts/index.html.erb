<h1>Blog On Rails</h1>
<div class="row">
        <% @posts.each do |post|%>
            <div class="col-sm-6 mb-4">
                <div class="card h-100">
                    <div class="card-body">
                    <h2><%= link_to(post.title, post_path(post.id) )%></h2>
                    <p> <%= post.body %></p>
                </div>
            </div>
        </div>
        <% end %>
    </div>
</div>